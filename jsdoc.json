{
    "source": {
        "include": ["./lib"],
        "includePattern": ".js$",
        "exclude": "(node_modules/docs)"
    },
    "plugin": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    }
}